<form class="listItem" action="seasons/<%= season.seasonId %>" method="get">
    <button class="playerButton" style="height: 100%;">
        <div class="itemFormat primaryStyle" style="height: 100%; border-style:solid; border-width: thick;">
            <div style="width: 100%;">
                <div class="playerName" style="text-align: center;">
                    <%= season.seasonName %>
                </div>
            </div>
            <div style="width: 100%; height: 100%; text-align: center;">
                <%= season.active == 1 ? 'Active' : 'Inactive' %>
            </div>
        </div>
    </button>
</form>