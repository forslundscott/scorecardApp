                <option value="<%= data.roles[i].id%>"><%= data.roles[i].name%></option>
            <% } %>
        </select>
        <br>
        <br>
        <button type="submit" class="primaryStyle primaryBorder" style="font-weight: bold; width: auto; height: 5vh;">Add Role</button>
    </form>
</body>
</html>