<body>
    <form id="newUserRoleForm" class="textCenter" action="addRole" method="post" style="display: ''; text-align: center;">
        <select name="roleId" class="primaryStyle" style="font-size: 100%;" autocomplete="off" onchange="" required>
            <option value="" disabled selected>Role</option>
            <!-- <script>console.log(document.getElementById('newGameForm').getElementsByName('leagueId')[0])</script> -->
